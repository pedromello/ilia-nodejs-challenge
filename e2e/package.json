{
    "name": "e2e",
    "private": true,
    "scripts": {
        "test": "jest --runInBand --forceExit",
        "test:watch": "jest --watch"
    },
    "devDependencies": {
        "@types/async-retry": "^1.4.9",
        "@types/jest": "^30.0.0",
        "@types/node": "^22.10.7",
        "async-retry": "^1.3.3",
        "jest": "^30.0.0",
        "ts-jest": "^29.2.5",
        "ts-node": "^10.9.2",
        "typescript": "^5.7.3"
    },
    "jest": {
        "rootDir": ".",
        "testRegex": ".*\\.test\\.ts$",
        "transform": {
            "^.+\\.ts$": "ts-jest"
        },
        "testEnvironment": "node",
        "testTimeout": 120000
    }
}
